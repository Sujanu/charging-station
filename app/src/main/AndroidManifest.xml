<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChargingStation">
        <activity
            android:name=".activites.GPSFetcher"
            android:exported="false"
            android:label="@string/title_activity_gpsfetcher"
            android:theme="@style/Theme.ChargingStation" />
        <activity
            android:name=".activites.StationDesc"
            android:exported="false"
            android:label="@string/title_activity_station_desc"
            android:theme="@style/Theme.ChargingStation" />
        <activity
            android:name=".activites.Charging"
            android:exported="false"
            android:label="@string/title_activity_charging"
            android:theme="@style/Theme.ChargingStation" />
        <activity
            android:name=".activites.Station3"
            android:exported="false"
            android:label="@string/title_activity_station3"
            android:theme="@style/Theme.ChargingStation" />
        <activity
            android:name=".activites.Station2"
            android:exported="false"
            android:label="@string/title_activity_station2"
            android:theme="@style/Theme.ChargingStation" />
        <activity
            android:name=".activites.Station1"
            android:exported="false"
            android:label="@string/title_activity_station1"
            android:theme="@style/Theme.ChargingStation" />
        <activity
            android:name=".activites.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ChargingStation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>